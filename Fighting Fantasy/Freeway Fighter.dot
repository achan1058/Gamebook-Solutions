	// graph defaults
	rankdir=LR
	mclimit=50
	nodesep=.3 // changes horizontal separation of nodes
	ranksep=.4 // changes vertical separation of nodes
	node [height=.7
		width=.7
		shape=ellipse
		margin=0]
	edge[weight=5]
	
	subgraph ShopMenu {
		node [fontcolor = "purple" shape = "rect" margin = 0.1]
